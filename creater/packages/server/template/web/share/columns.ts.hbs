import type { Column } from '@/chant'

export default () => {
  return [
  {{#each list}}
    {
      prop: '{{columnName}}',
      label: '{{columnComment}}',
      {{#if required}}
      required: '{{required}}',
      {{/if}}
      {{#if type}}
      type: '{{type}}',
      {{/if}}
    }{{#if @last}}{{else}},{{/if}}
  {{/each}}
  ] as Column[]
}
